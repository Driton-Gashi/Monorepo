{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/dist/index.js"
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "client/_next/static/$1"
    },
    {
      "src": "/_next/data/(.*)",
      "dest": "client/_next/data/$1"
    },
    {
      "src": "/_next/image(.*)",
      "dest": "client/_next/image$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
